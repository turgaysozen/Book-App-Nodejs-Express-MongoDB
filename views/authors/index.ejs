<h2>Search Authors</h2>
<form action="/authors" method="GET">
<label>Name</label>
<input type="text" name="name" value="<%= searchObject.name%>">
<button type="submit">Search</button>
</form>
<br>
<%let i = 0;%>
<% authors.forEach(author => { %>
    <%i++%>
    <div><%=i + ' - ' + author.name%></div>
<%})%>